{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: App",
            "type": "python",
            "request": "launch",
            "program": "app.py",
            "console": "integratedTerminal",
            "env": {
                "ENVIRONMENT_FILE": ".env.dev"
